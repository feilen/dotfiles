#!/bin/sh
xrandr --output DVI-I-3 --off --output HDMI-0 --off
xrandr --output DVI-I-3 --off --output HDMI-0 --primary --rotate left --mode 1080x1920 --rate 75 --auto --output DVI-I-2 --mode 1152x864 --rate 75 --auto --same-as HDMI-0 --scale-from 1920x1080
dispwin -d1 /home/feilen/.config/asus_vh222t.icm
